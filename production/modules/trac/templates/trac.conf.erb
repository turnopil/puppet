WSGIScriptAlias /trac <%= @trac_envpath %>/<%= @trac_project_name %>/cgi-bin/trac.wsgi
<Directory <%= @trac_envpath %>/<%= @trac_project_name %>/cgi-bin>
    WSGIApplicationGroup %{GLOBAL}
    Order deny,allow
    Allow from all
</Directory>
 
<Location "/trac">
    AuthType Basic
    AuthName "trac"
    AuthUserFile /var/trac/users/htpasswd
    Require valid-user
</Location>